                        <tr>
                            @if ((Model.DCFHippaEndDate - DateTime.Now).TotalDays <= 0)
                            {
                                <th class="legend" colspan="3">
                                    <span class="expired"></span>DCF Hippa Expired
                                </th>
                            }
                            else if ((Model.DCFHippaEndDate - DateTime.Now).TotalDays <= 7)
                            {
                                <th class="legend" colspan="3">
                                    <span class="lessthan7"></span>DCF Hippa Expires in @((Model.DCFHippaEndDate - DateTime.Now).Days) Days
                                </th>
                            }
                            else if ((Model.DCFHippaEndDate - DateTime.Now).TotalDays <= 14)
